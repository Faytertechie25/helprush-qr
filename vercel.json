{
  "routes": [
    {
      "src": "^/$",
      "status": 308,
      "headers": {
        "Location": "/qr"
      },
      "has": [
        {
          "type": "host",
          "value": "verify.helprush.in"
        }
      ]
    }
  ],
  "headers": [
    {
      "source": "/qr",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=300, stale-while-revalidate=60"
        }
      ]
    }
  ]
}
